{
  "build": {
    "content": [
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "about.md",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "JeremyTCD",
      "_appFooter": "<span>Copyright Â© 2017 JeremyTCD</span>",
      "_enableSearch": true
    },
    "fileMetadata": {
      "_tocRel" : {
        "index.md": "articles/toc.html" 
      }
    },
    "theme": [
      "default",
      "customTheme"
    ],
    "template": [
      "default",
      "customTemplate"
    ],
    "postProcessors": [],
    "noLangKeyword": false
  }
}