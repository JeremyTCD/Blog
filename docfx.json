{
  "build": {
    "content": [
      {
        "src": "src",
        "files": [
          "**"
        ],
        "exclude": [
          "**/*.json"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "JeremyTCD",
      "_appFooter": "<span>Copyright Â© 2017 JeremyTCD</span>",
      "_enableSearch": false,
      "jr.enableSearch": true
    },
    "template": [
      "DocFxBlogTemplate"
    ],
    "postProcessors": [ "SortedArticleListGenerator", "SearchIndexGenerator" ],
    "noLangKeyword": false
  }
}